{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "Front_PoulinaFutureJobs": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "inlineStyle": true,
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/front-poulina-future-jobs",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              },
              {
                "glob": "**/*",
          "input": "src/assets",
          "output": "assets"
              }
            ],
            "styles": [
              "src/styles.css",
        "node_modules/ngx-toastr/toastr.css",
        "src/assets/css/fontawesome.css",
        "src/assets/css/vendors/icofont.css",
        "src/assets/css/vendors/themify.css",
        "src/assets/css/vendors/flag-icon.css",
        "src/assets/css/vendors/feather-icon.css",
        "src/assets/css/vendors/bootstrap.css",
        "src/assets/css/style.css",
        "src/assets/css/color-1.css",
        "src/assets/css/responsive.css",
       "src/assets/css/vendors/slick.css",
        "src/assets/css/vendors/slick-theme.css",
        "src/assets/css/vendors/scrollbar.css",
        "src/assets/css/vendors/animate.css",
          "src/assets/css/vendors/jquery.dataTables.css"
            ],
            "scripts": [
             "src/assets/js/jquery.min.js",
        "src/assets/js/bootstrap/bootstrap.bundle.min.js",
        "src/assets/js/icons/feather-icon/feather.min.js",
        "src/assets/js/icons/feather-icon/feather-icon.js",
        "src/assets/js/scrollbar/simplebar.min.js",
        "src/assets/js/scrollbar/custom.js",
        "src/assets/js/config.js",
    "src/assets/js/sidebar-menu.js",
        "src/assets/js/sidebar-pin.js",
        "src/assets/js/clock.js",
        "src/assets/js/slick/slick.min.js",
        "src/assets/js/slick/slick.js",
        "src/assets/js/header-slick.js",
        "src/assets/js/chart/apex-chart/apex-chart.js",
        "src/assets/js/chart/apex-chart/stock-prices.js",
        "src/assets/js/counter/counter-custom.js",
        "src/assets/js/notify/bootstrap-notify.min.js",
        "src/assets/js/dashboard/default.js",
        "src/assets/js/notify/index.js",
        "src/assets/js/datatable/datatables/jquery.dataTables.min.js",
        "src/assets/js/datatable/datatables/dataTables.js",
        "src/assets/js/datatable/datatables/dataTables.select.js",
        "src/assets/js/datatable/datatables/select.bootstrap5.js",
        "src/assets/js/datatable/datatables/datatable.custom.js",
        "src/assets/js/typeahead/handlebars.js",
        "src/assets/js/typeahead/typeahead.bundle.js",
        "src/assets/js/typeahead/typeahead.custom.js",
        "src/assets/js/typeahead-search/handlebars.js",
        "src/assets/js/typeahead-search/typeahead-custom.js",
        "src/assets/js/script.js",
        "src/assets/js/script1.js",
        "src/assets/js/theme-customizer/customizer.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.development.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "Front_PoulinaFutureJobs:build:production"
            },
            "development": {
              "buildTarget": "Front_PoulinaFutureJobs:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              },
              {
                "glob": "**/*",
          "input": "src/assets",
                "output": "assets"
              }
            ],
            "styles": [
              "src/styles.css",
              "node_modules/ngx-toastr/toastr.css",
        "src/assets/css/fontawesome.css",
        "src/assets/css/vendors/icofont.css",
        "src/assets/css/vendors/themify.css",
        "src/assets/css/vendors/flag-icon.css",
        "src/assets/css/vendors/feather-icon.css",
        "src/assets/css/vendors/bootstrap.css",
        "src/assets/css/style.css",
        "src/assets/css/color-1.css",
        "src/assets/css/responsive.css",
        "src/assets/css/vendors/slick.css",
        "src/assets/css/vendors/slick-theme.css",
        "src/assets/css/vendors/scrollbar.css",
        "src/assets/css/vendors/animate.css",
          "src/assets/css/vendors/jquery.dataTables.css"
            ],
            "scripts": [
        "src/assets/js/jquery.min.js",
        "src/assets/js/bootstrap/bootstrap.bundle.min.js",
        "src/assets/js/icons/feather-icon/feather.min.js",
        "src/assets/js/icons/feather-icon/feather-icon.js",
        "src/assets/js/scrollbar/simplebar.min.js",
        "src/assets/js/scrollbar/custom.js",
        "src/assets/js/config.js",
        "src/assets/js/sidebar-menu.js",
        "src/assets/js/sidebar-pin.js",
        "src/assets/js/clock.js",
        "src/assets/js/slick/slick.min.js",
        "src/assets/js/slick/slick.js",
        "src/assets/js/header-slick.js",
        "src/assets/js/chart/apex-chart/apex-chart.js",
        "src/assets/js/chart/apex-chart/stock-prices.js",
        "src/assets/js/counter/counter-custom.js",
        "src/assets/js/notify/bootstrap-notify.min.js",
        "src/assets/js/dashboard/default.js",
        "src/assets/js/notify/index.js",
        "src/assets/js/datatable/datatables/jquery.dataTables.min.js",
        "src/assets/js/datatable/datatables/dataTables.js",
        "src/assets/js/datatable/datatables/dataTables.select.js",
        "src/assets/js/datatable/datatables/select.bootstrap5.js",
        "src/assets/js/datatable/datatables/datatable.custom.js",
        "src/assets/js/typeahead/handlebars.js",
        "src/assets/js/typeahead/typeahead.bundle.js",
        "src/assets/js/typeahead/typeahead.custom.js",
        "src/assets/js/typeahead-search/handlebars.js",
        "src/assets/js/typeahead-search/typeahead-custom.js",
        "src/assets/js/script.js",
        "src/assets/js/script1.js",
        "src/assets/js/theme-customizer/customizer.js"
            ]
          }
        }
      }
    }
  }
}